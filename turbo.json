{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",

  "pipeline": {
    "build": {
      "outputs": [".solid/**", "dist/**"],
      "dependsOn": ["$OS", "$NODE_VERSION 18"]
    },
    "test": {
      "dependsOn": ["build", "$OS", "$NODE_VERSION"],
      "outputs": []
    },
    "dev": {
      "cache": true if required 
      "cache": false if not required 
    }
  }
}
